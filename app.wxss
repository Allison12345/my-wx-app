/* 左右顶格 上下居中对齐 */
.row {
  flex-direction: row;
}
.column {
  flex-direction: column;
}
.flex-x-sb {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.flex-x-cc {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
page {
  width: 100%;
  height: 100%;
}

.full {
  width: 100%;
  height: 100%;
}
