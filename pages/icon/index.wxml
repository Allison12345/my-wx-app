<my-page>
  <view class="group">
    <block wx:for="{{ iconSize }}" wx:key="*this">
      <icon type="success" size="{{ item }}"></icon>
    </block>
  </view>
  <view class="group">
    <block wx:for="{{ iconColor }}" wx:key="*this">
      <icon type="success" color="{{ item }}"></icon>
    </block>
  </view>
  <view class="group">
    <block wx:for="{{ iconType }}" wx:key="*this">
      <icon type="{{ item }}"></icon>
    </block>
  </view>
  <view class="group">
    <block wx:for="{{ myIcon }}" wx:key="*this">
      <icon
        type="{{ item.type }}"
        size="{{ item.size }}"
        color="{{ item.color }}"
      ></icon>
    </block>
  </view>
</my-page>
